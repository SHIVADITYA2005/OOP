#include <iostream>
using namespace std;

class Dispenser {
public:
    int a,b,c,d,e,f,g,h,j;
    int search;
    int initial() {
        int N;
        cout << "Select group of objects by number:" << endl;
        cout << "1. TOOLS" << endl;
        cout << "2. SPARES" << endl;
        cout << "3. MEDICINES" << endl;
        cout << "4. CONSUMEABLES" << endl;
        cout << "5. MANNUALS" << endl;
        cin >> N;
        return N;
    }
};
class TOOLS :public Dispenser {
public:
    int n;
    int tool(){
        cout<<"Enter the quantity of Mechanical tool"<<endl;
        cin>>a;
        cout<<"Enter the quantity of Electrical tool"<<endl;
        cin>>b;
        cout<<"Enter the quantity of Diagnostic tool"<<endl;
        cin>>c;
        cout<<"Enter the quantity of Robotic tool"<<endl;
        cin>>d;
        cout<<"Enter the quantity of Emergency tool"<<endl;
        cin>>e;
        cout<<"Enter the quantity of Extravehicular tool"<<endl;
        cin>>f;
        cout << "Which tool do you want:" << endl;
        cout << "11. Mechanical" << endl;
        cout << "12. Electrical" << endl;
        cout << "13. Diagnostic Tools" << endl;
        cout << "14. Robotic Tools" << endl;
        cout << "15. Emergency Tools" << endl;
        cout << "16. Extravehicular Activity (EVA) Tools" << endl;
        cin >> n;
        cout<<"Enter the quantity of item you want"<<endl;
        cin>>search;
        switch(n){
            case 11:{
                if(a>=search){
                a-=search;
                return n;
                }
        else
        cout<<"The quantity is Insufficient"<<endl;
        break;
        }
            case 12:{
                if(b>=search){
                b-=search;
                return n;
                }
        else
        cout<<"The quantity is Insufficient"<<endl;
        break;
        }
              case 13:{
                if(c>=search){
                c-=search;
                return n;
                }
        else
        cout<<"The quantity is Insufficient"<<endl;
        break;
        }
              case 14:{
                if(d>=search){
                d-=search;
                return n;
                }
        else
        cout<<"The quantity is Insufficient"<<endl;
        break;
        }
              case 15:{
                if(e>=search){
                e-=search;
                return n;
                }
        else
        cout<<"The quantity is Insufficient"<<endl;
        break;
        }
              case 16:{
                if(f>=search){
                f-=search;
                return n;
                }
        else
        cout<<"The quantity is Insufficient"<<endl;
        break;
        }
        }
        
    }    
};

